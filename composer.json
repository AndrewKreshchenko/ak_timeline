{
    "name": "ak/ak-timelinevis",
    "type": "typo3-cms-extension",
    "description": "TYPO3 extension allows to create and manage timeline and widgets, provides interactive visual representations of detailed historic data, and revision timeline in arbitrary date intervals.",
    "license": [],
    "authors": [
        {
            "name": "Andrii Kreshchenko",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "mail2andyk@gmail.com",
        "irc": "irc://irc.freenode.org/composer"
    },
    "version": "1.0.0",
    "require": {
        "typo3/cms-core": "^10.4 || ^11.5"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "ak_timeline"
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "AK\\TimelineVis\\": "Classes"
        }
    }
}
